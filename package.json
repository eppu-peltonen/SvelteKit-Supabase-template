	"dependencies": {
		"@supabase/auth-helpers-sveltekit": "^0.10.0",
		"@supabase/auth-ui-shared": "^0.1.6",
		"@supabase/auth-ui-svelte": "^0.2.2",
		"@supabase/supabase-js": "^2.23.0",
		"@types/bcrypt": "^5.0.0",
		"bcrypt": "^5.1.0",
		"carbon-icons-svelte": "^11.4.0",
		"classnames": "^2.3.2",
		"svelte-legos": "^0.2.1"
	}
}
